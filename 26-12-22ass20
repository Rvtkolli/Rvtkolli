def isprime(n):
 for i in range(1,n+1):
   if n%i==0:
      return isprime
n=int(input ())
num=int(input ('enter a number'))
maximum=num
minimum=maximum
for i in range(1,n):
  num=int(input('enter a no'))
  if num>maximum:
      maximum=num
  if num<minimum:
      minimum=num
sum=isprime(maximum)
sum2=isprime(minimum)
print('maximum no is',max)
print('minimum no is',min)
