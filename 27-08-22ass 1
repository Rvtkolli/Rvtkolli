
#include <stdio.h>
#include<string.h>
int main() {
    char  str[20],str1[20];
    int i,j,n,n1,count=0;
    char temp;
    printf( "enter first string\n");
    gets(str);
    printf( "enter second string\n");
    gets(str1);
    n=strlen(str);
    for(i=0;i<n-1;i++)
    {
        for(j=0;j<n-i-1;j++)
        {
            if(str[j]>str[j+1])
            {
                temp=str[j];
                str[j]=str[j+1];
                str[j+1]=temp;
            }
        
     if(str1[j]>str1[j+1])
            {
                temp=str1[j];
                str1[j]=str1[j+1];
                str1[j+1]=temp;
            }
        }
    }
    
    for(i=0;str[i]!='\0';i++)
    {
        
        if(str[i]==str1[i])
            count++;
    }
    if(count==n)
    printf( "Anagram");
    else
    printf( "not anagram");
    return 0;
}
