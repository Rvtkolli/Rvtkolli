
#include <stdio.h>
#include<string.h>
int main() {
  char str[20];
  int i,j,x,n,k;
  printf( "enter string\n");
  gets(str);
  n=strlen(str);
  for(i=1;i<=n;i++)
  {
      for(j=0;j<=n-i;j++)
      {
         int x=j+i;
         for(k=j;k<x;k++)
         {
             printf( "%c",str[k]);
         }
         printf( "\n");
      }
  }
    return 0;
}
